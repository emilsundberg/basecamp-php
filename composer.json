{
    "name": "emilsundberg/basecamp-php",
    "description": "API Wrapper for Basecamp 4",
    "require": {
        "php": ">=7.0",
        "ext-fileinfo": "*",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "kevinrob/guzzle-cache-middleware": "^3.0|^4.0",
        "illuminate/cache": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^6.5 || ^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Emil Sundberg",
            "email": "mbastien@coopbelvedere.com"
        },
        {
            "name": "Martin Bastien",
            "email": "mbastien@coopbelvedere.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Emil\\Basecamp\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emil\\Basecamp\\BasecampServiceProvider"
            ],
            "aliases": {
                "Basecamp": "Emil\\Basecamp\\BasecampFacade"
            }
        }
    }
}
